#include<stdio.h>
#include<math.h>
int isprime(int x){ if(x<2) return 0; for(int i=2;i<=sqrt(x);i++) if(x%i==0) return 0; return 1; }
int main(){
    int a[]={2,3,4,5,6,7}, n=6, cnt=0;
    for(int i=0;i<n;i++) if(isprime(a[i])) cnt++;
    printf("Prime percentage = %f%%\n", (cnt*100.0)/n);
    return 0;
}
