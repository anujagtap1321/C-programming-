#include <stdio.h>
int main(){
    double m1,m2,m3,m4,m5,total;
    printf("Enter marks (5 subjects) and total marks: ");
    if(scanf("%lf %lf %lf %lf %lf %lf",&m1,&m2,&m3,&m4,&m5,&total)!=6) return 0;
    double sum = m1+m2+m3+m4+m5;
    printf("Percentage = %f%%\n", sum/total*100.0);
    return 0;
}
